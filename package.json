{
  "name": "focus",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "16.8.5",
    "react-scripts": "2.1.8",
    "axios": "0.18.0",
    "babel-polyfill": "6.26.0",
    "classnames": "2.2.6",
    "connected-react-router": "6.3.2",
    "history": "4.7.2",
    "immutable": "4.0.0-rc.12",
    "node-sass": "4.11.0",
    "prop-types": "15.7.2",
    "react-content-loader": "4.2.1",
    "react-dates": "18.2.2",
    "react-moment-proptypes": "1.6.0",
    "react-redux": "6.0.1",
    "react-router-dom": "4.3.1",
    "react-tracking": "5.7.0",
    "redux": "4.0.1",
    "redux-immutable": "4.0.0",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "@typescript-eslint/indent": false,
      "@typescript-eslint/explicit-function-return-type": false,
      "@typescript-eslint/explicit-member-accessibility": false,
      "@typescript-eslint/no-use-before-define": false,
      "@typescript-eslint/no-var-requires": false,
      "@typescript-eslint/prefer-interface": false,
      "@typescript-eslint/no-object-literal-type-assertion": false,
      "no-unused-vars": "error",
      "react/prop-types": "error",
      "react/no-unused-prop-types": "error",
      "no-console": "error"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "react-redux": "^6.0.1",
    "@types/classnames": "2.2.7",
    "@types/enzyme": "3.1.18",
    "@types/file-saver": "2.0.0",
    "@types/history": "4.7.2",
    "@types/jest": "24.0.6",
    "@types/lodash": "4.14.121",
    "@types/lolex": "3.1.1",
    "@types/prop-types": "15.5.9",
    "@types/query-string": "6.2.0",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.2",
    "@types/react-redux": "7.0.1",
    "@types/react-router": "4.4.4",
    "@types/redux-immutable": "4.0.1",
    "@typescript-eslint/eslint-plugin": "1.3.0",
    "@typescript-eslint/parser": "1.3.0",
    "axios-mock-adapter": "1.16.0",
    "cross-env": "5.2.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "eslint-config-react-app": "3.0.8",
    "eslint-plugin-react": "7.12.4",
    "jest-enzyme": "7.0.2",
    "jest-immutable-matchers": "2.0.1",
    "jest-mock-axios": "2.3.0",
    "lolex": "3.1.0",
    "query-string": "6.4.0",
    "redux-devtools-extension": "2.13.8",
    "stylelint": "9.10.1",
    "stylelint-config-standard": "18.2.0",
    "stylelint-scss": "3.5.4",
    "typescript": "3.3.3"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
